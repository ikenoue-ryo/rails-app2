
<%= form_with(model: @plan, local: true) do |f| %>
  <div class="">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div class="">
    <%= f.label :start_time %>
    <%= f.text_field :start_time %>
  </div>
  
  <div class="">
    <%= f.label :content %>
    <%= f.text_field :content %>
  </div>

  <div class="">
    <%= f.submit %>
  </div>
<% end %>